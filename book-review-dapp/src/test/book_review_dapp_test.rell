@test module;

import main.*;

function test_add_book() {
    rell.test.tx()
             .op(create_book("123", "book1", "author1"))
             .op(create_book("124", "book2", "author2"))
             .run();

    val all_books = book @* {};

    assert_equals(all_books.size(), 2);
    assert_equals(all_books[0].title, "book1");
    assert_equals(all_books[1].author, "author2");
}
